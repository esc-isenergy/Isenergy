@model IsEnergyModel.PhysicalPersons
@{
    ViewBag.Title = "Детали физ лица";
}
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<div class="align_center_to_left">
    <div class="align_center_to_right">
        <div class="align_centerInCss">
            <h3>Детали</h3>
            @Html.DevExpress().PageControl(
    settings =>
    {  
        settings.Name = "pageControl";

        settings.TabPages.Add("Реквизиты", "Requisite").SetContent(() =>
        {
            Html.RenderPartial("Details/Requisite/_RequisitePartial", Model);
        });
        settings.TabPages.Add("Сервис", "Access").SetContent(() =>
        {
            Html.RenderAction("Changes", "Subscriber", new { IdentifierSubscriber = Model.IdentifierSubscriber });
        });
        settings.TabPages.Add("История", "History").SetContent(() =>
        {
            Html.RenderAction("History", "Subscriber", new { IdentifierSubscriber = Model.IdentifierSubscriber });
        });
    }).GetHtml()
</div>
            <div>

                @Html.ActionLink("Назад", "Index")
            </div>
        
    </div>
</div>

    




